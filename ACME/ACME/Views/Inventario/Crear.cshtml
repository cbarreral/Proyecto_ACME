@model InventarioModel

<form asp-action="Crear" method="post">
    <div class="form-group">
        <label asp-for="SucursalId">ID sucursal</label>
        <input asp-for="SucursalId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ProductoId">Producto</label>
        <input asp-for="ProductoId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CantidadTotalDeProductosDisponibles">Cantidad</label>
        <input asp-for="CantidadTotalDeProductosDisponibles" class="form-control" />

    </div>
    <button type="submit" class="btn btn-primary">Crear</button>
</form>